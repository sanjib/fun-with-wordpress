{
  "name" 			: "oak/wordpress-starter-plugin",
  "description" 	: "Oak Starter is a sandbox WordPress plugin.",
  "type"            : "project",
  "keywords" 		: ["wordpress", "wp", "plugin", "starter-plugin"],
  "homepage" 		: "https://oak.dev/wp/starter",
  "license" 		: "GPL-2.0+",
  "authors": [
    {
      "name" 	: "Sanjib Ahmad",
      "email" : "sanjib@sanjib.org"
    }
  ],
  "require": {
    "php" : ">=5.4.0"
  },
  "require-dev": {
    "kint-php/kint": "^1",
    "filp/whoops": "^2"
  },
  "autoload" : {
    "classmap": [],
    "files": [
      "src/support/exceptions.php",
      "src/sandbox.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "vendor-dir": "assets/vendor"
  },
  "minimum-stability": "dev"
}